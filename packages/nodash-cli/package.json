{
  "name": "@nodash/cli",
  "version": "0.1.0",
  "description": "CLI tool for nodash - developer layer",
  "main": "dist/cli.js",
  "types": "dist/cli.d.ts",
  "bin": {
    "nodash": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "nodash",
    "cli",
    "developer-tools"
  ],
  "author": "Nodash Team",
  "license": "MIT",
  "dependencies": {
    "@nodash/sdk": "^0.1.0",
    "commander": "^11.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "vitest": "^1.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ]
}
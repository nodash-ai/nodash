{
  "name": "@nodash/cli",
  "version": "1.0.1",
  "description": "Nodash CLI for project analysis and AI-assisted analytics setup",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "nodash": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "start": "node dist/cli.js",
    "test": "vitest run",
    "lint": "eslint src",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "analytics",
    "cli",
    "ai",
    "mcp"
  ],
  "author": "Nodash Team",
  "license": "MIT",
  "dependencies": {
    "@nodash/sdk": "^1.0.0",
    "chalk": "^5.4.1",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.2",
    "typescript": "^5.0.0",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

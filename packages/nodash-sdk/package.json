{
  "name": "@nodash/sdk",
  "version": "0.1.0",
  "description": "Minimal SDK for nodash - the foundation layer",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "node scripts/build-dual.js",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run --reporter=verbose --pool=threads",
    "typecheck": "tsc --noEmit --incremental"
  },
  "keywords": [
    "nodash",
    "sdk",
    "analytics",
    "tracking",
    "web",
    "browser",
    "esm"
  ],
  "author": "Nodash Team",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "typescript": "^5.0.0",
    "vitest": "^4.0.0-beta.3",
    "@types/node": "^20.0.0",
    "jsdom": "^23.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ]
}